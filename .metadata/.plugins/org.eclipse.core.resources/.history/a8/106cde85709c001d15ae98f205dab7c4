package chapter01;

public class Test01 {
	public static int solution(int n) {
		int answer = 0;
		for (int i = 1; i <= n; i++) {
			int a = n%i;
			System.out.println(a);
			int[] arr1 = new int[n];
			if (a == 0) {
				for (int j = 0; j < arr1.length; j++) {
					arr1[j] = i; // 0번째 자리가 1
					System.out.println(arr1[0]);
				}
			}
			
		}
		return answer;
	}
	public static void main(String[] args) {
		solution(5);
	}
}
