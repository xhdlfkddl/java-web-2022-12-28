package chapter01;

public class Test01 {
	public static int solution(int n) {
		int answer = 0;
		int count = 0;
		int[] arr1 = new int[count];
		
		for (int i = 1; i <= n; i++) {
			int a = n%i;
//			System.out.println(a);
			if (a == 0) {
				count += 1;
				System.out.println(count);
//				for (int j = 0; j < arr1.length; j++) {
//					arr1[j] = i; // 0번째 자리가 1
//					answer += arr1[j];
//				}
			}
			
		}
		return answer;
	}
	public static void main(String[] args) {
		solution(5);
	}
}
