package chapter01;

import java.util.Iterator;

public class Test04 {
	public static int[] solution(long n) {
		int[] answer = {};
		long num = n;
		int cnt = 0;
		
		while (num != 0) {
			num /= 10;
			cnt++;
			System.out.println(num);
			System.out.println(cnt);
		}
		
        return answer;
    }
	
	public static void main(String[] args) {
		solution(1234);
	}
}
